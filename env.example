# Direct Access for Farmers - Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Configuration
# Your backend API base URL (when you have a backend)
VITE_API_BASE_URL=http://localhost:3000/api

# Socket.io Configuration
# Your Socket.io server URL (when you have a backend)
VITE_SOCKET_URL=http://localhost:3001

# Razorpay Configuration
# Your Razorpay key ID (from Razorpay dashboard)
# Format: rzp_test_xxxxxxxxxxxxx or rzp_live_xxxxxxxxxxxxx
VITE_RAZORPAY_KEY=rzp_test_your_key_here

# Application Configuration
# App title (optional)
VITE_APP_TITLE=Direct Access for Farmers

# Environment
# development | production
VITE_ENV=development







